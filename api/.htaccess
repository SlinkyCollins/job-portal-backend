RewriteEngine On
RewriteBase /JobPortal/api/

# Specific rules for dynamic routes (add more as needed)
RewriteRule ^jobs/([0-9]+)$ jobs/jobdetails.php?id=$1 [L,QSA]

# General rule: Remove .php extension for static routes
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.+)$ $1.php [L,QSA]